    "import echo\n",
    "\n",
    "alt_rows0 = [echo.hwells[(ii:=i*24):ii+24] for i in range(0,17,2)]\n",
    "alt_rows1 = [echo.hwells[(ii:=i*24):ii+24] for i in range(1,17,2)]\n",
    "r0 = [j for i in alt_rows0 for j in i][::2]\n",
    "r1 = [j for i in alt_rows0 for j in i][1::2]\n",
    "r2 = [j for i in alt_rows1 for j in i][::2]\n",
    "r3 = [j for i in alt_rows1 for j in i][1::2]\n",
    "\n",
    "#r0 = [i for j in alt_rows0[::2] for i in j[::2]]\n",
    "#r1 = [i for j in alt_rows0[1::2] for i in j[1::2]]\n",
    "#r2 = [i for j in alt_rows1[::2] for i in j[::2]]\n",
    "#r3 = [i for j in alt_rows1[1::2] for i in j[1::2]]\n",
    "\n",
    "racks = {i:df.loc[df['Rack Number']==i,:] for i in df['Rack Number'].unique()}\n",
    "\n",
    "src_plates = []\n",
    "dest_plates = []\n",
    "\n",
    "for rk, df_, well_allocs in zip(list(racks.keys())[:4], \n",
    "                               racks.values(),\n",
    "                               [r0,r1,r2,r3]):\n",
    "    \n",
    "    src = echo.SrcPlate(name=f'src-{rk}',ldv=True)\n",
    "    dest = echo.Plate(name=f'dest-{rk}')\n",
    "    for i,j,k in zip(well_allocs,\n",
    "                     df_['CatalogNumber'],\n",
    "                     df_['Plate Location']):\n",
    "        src_well = src[i]\n",
    "        dest_well = dest[i]\n",
    "        src_well.fill(echo.Cpd(j,vol=10))\n",
    "        src_well.xfer(dest_well,1.5)\n",
    "    src_plates.append(src)\n",
    "    dest_plates.append(dest)\n",
    "    \n",
    "picklist = pd.concat([pd.DataFrame(i.xfer_record) for i in src_plates])\n",
    "picklist.to_csv('00-picklist.csv',index=False)\n",
    "picklist.head()"
   ]
